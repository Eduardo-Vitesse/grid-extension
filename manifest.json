{
    "manifest_version": 2,
    "name": "Auto Scroll",
    "version": "0.1.0",
    "icons": {
        "16": "./icons/icon_16.png",
        "24": "./icons/icon_24.png",
        "32": "./icons/icon_32.png",
        "64": "./icons/icon_64.png",
        "128": "./icons/icon_128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./js/content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "./icons/icon_128.png",
        "default_popup": "./pages/popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}